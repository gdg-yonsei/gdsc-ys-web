{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "App.tsx",
      "options": {
        "printWidth": 180
      }
    },
    {
      "files": "*.html",
      "options": {
        "printWidth": 300
      }
    }
  ]
}
