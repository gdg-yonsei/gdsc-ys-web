{
  "name": "@gdsc-ys/color",
  "description": "Color system for GDSC Yonsei & other chapters",
  "version": "0.0.1",
  "main": "dist/index.js",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "ready": "yarn build && yarn sortpackage",
    "build": "yarn build:clear && yarn build:types",
    "build:clear": "rm -rf dist",
    "build:types": "tsc -b && tsc-alias",
    "sortpackage": "ts-node --project ./scripts/tsconfig.node.json ./scripts/sort-package.ts"
  },
  "devDependencies": {
    "@gdsc-ys/misc": "workspace:^",
    "@types/node": "^18.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^7.6.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": "18.2.*"
  }
}
