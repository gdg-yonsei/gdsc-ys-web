{
  "name": "gdsc-web",
  "description": "Monolithic repository config for gdsc-yonsei web apps & packages",
  "packageManager": "yarn@3.6.3",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "notion": "yarn workspace @gdsc-ys/notion",
    "notion:dev": "yarn workspace @gdsc-ys/notion dev",
    "misc": "yarn workspace @gdsc-ys/misc"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "18.7.1",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vercel/node": "^3.0.3",
    "@yarnpkg/pnpify": "^3.1.5",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "^2.7.1",
    "tsc-alias": "^1.8.7",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
